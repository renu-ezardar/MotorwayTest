{
  "name": "mw-backend-test",
  "main": "dist/server.mjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev vite",
    "build": "vite build --outDir dist --ssr src/server.ts",
    "format": "prettier --config .prettierrc \"src/**/*.{js,json,ts,css,scss,vue,html,md}\" --write",
    "lint": "eslint . --fix",
    "start": "NODE_ENV=prod node dist/server.mjs",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "third-party-api:serve-docs": "node third-party-api/docs.js"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "dotenv": "16.5.0",
    "fastify": "5.3.3",
    "mw-backend-test": "file:",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "reflect-metadata": "0.2.2",
    "sqlite3": "5.1.7",
    "typeorm": "0.3.24",
    "typeorm-fastify-plugin": "3.0.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.27.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@swc/core": "1.11.29",
    "@types/node": "22.15.21",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/ui": "3.1.4",
    "c8": "10.1.3",
    "cross-env": "^7.0.3",
    "eslint": "9.27.0",
    "jsdom": "26.1.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1",
    "unplugin-swc": "1.5.3",
    "vite": "6.3.5",
    "vite-plugin-node": "5.0.1",
    "vitest": "3.1.4"
  },
  "overrides": {
    "eslint": "9.27.0",
    "vite": "6.3.5",
    "vitest": "3.1.4"
  }
}
